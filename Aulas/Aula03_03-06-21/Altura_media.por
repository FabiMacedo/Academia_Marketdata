programa {
	funcao inicio() {
		real alt1, alt2, alt3, media
		
		escreva("Digite a altura do primeiro aluno: ")
		leia(alt1)
		escreva("Digite a altura do segundo aluno: ")
		leia(alt2)
		escreva("Digite a altura do terceiro aluno: ")
		leia(alt3)
		
		media = (alt1 + alt2 + alt3)/3
		
		escreva("A altura média dos alunos é: ", media)
		
		
	}
}
